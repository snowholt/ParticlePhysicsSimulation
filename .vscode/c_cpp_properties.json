{
    "configurations": [
        {
            "name": "Linux",
            "includePath": [
                "${workspaceFolder}/**",
                "${workspaceFolder}/include",
                "/usr/include",
                "/usr/local/include",
                "/usr/include/c++/11",
                "/usr/include/x86_64-linux-gnu",
                "/usr/include/x86_64-linux-gnu/c++/11",
                "/usr/lib/gcc/x86_64-linux-gnu/11/include"
            ],
            "defines": [
                "__AVX__",
                "__AVX2__"
            ],
            "compilerPath": "/usr/bin/g++",
            "cStandard": "c11",
            "cppStandard": "c++17",
            "intelliSenseMode": "linux-gcc-x64",
            "compilerArgs": [
                "-mavx",
                "-mavx2",
                "-O3",
                "-march=native",
                "-pthread"
            ],
            "browse": {
                "path": [
                    "${workspaceFolder}",
                    "${workspaceFolder}/include",
                    "/usr/include",
                    "/usr/local/include",
                    "/usr/include/c++/11",
                    "/usr/include/x86_64-linux-gnu",
                    "/usr/include/x86_64-linux-gnu/c++/11",
                    "/usr/lib/gcc/x86_64-linux-gnu/11/include"
                ],
                "limitSymbolsToIncludedHeaders": true,
                "databaseFilename": "${workspaceFolder}/.vscode/.browse.vc.db"
            }
        }
    ],
    "version": 4
}
